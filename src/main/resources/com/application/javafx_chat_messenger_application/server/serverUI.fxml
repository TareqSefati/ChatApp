<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.javafx_chat_messenger_application.server.ServerController2">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="labelInfo" />
            <HBox alignment="CENTER" prefHeight="32.0" prefWidth="600.0" spacing="104.0">
               <children>
                  <Label text="Total User List">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="12.0" />
                     </font>
                  </Label>
                  <Label text="Witty Server">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="23.0" />
                     </font>
                  </Label>
                  <Label text="Active Clients">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="12.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="200.0" prefWidth="600.0" spacing="10.0">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
               <children>
                  <ScrollPane fx:id="spUserList" prefHeight="200.0" prefWidth="244.0">
                     <content>
                        <VBox fx:id="usersBox" alignment="CENTER" prefHeight="197.0" prefWidth="241.0" />
                     </content>
                  </ScrollPane>
                  <ScrollPane fx:id="sp_main" prefHeight="200.0" prefWidth="331.0">
                     <content>
                        <VBox fx:id="activeClientsBox" alignment="CENTER" prefHeight="197.0" prefWidth="328.0" />
                     </content>
                  </ScrollPane>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="49.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <TextField fx:id="tf_message" prefHeight="31.0" prefWidth="508.0" />
                  <Button fx:id="button_send" mnemonicParsing="false" text="Send">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
